    <name>undoElementMovedOrRenamed-not-called-on-undo-file-move Template</name>
    <vendor>kmax</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.kmax.undoelementmovedorrenamednotcalledonundofilemove.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>
